<Window x:Class="DI_11.TaskInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DI_11"
        mc:Ignorable="d"
        Title="TaskInfo" Height="450" Width="800">
    <Grid Background="#AE8979">
    <Grid.RowDefinitions>
        <RowDefinition></RowDefinition>
        <RowDefinition Height="9*"></RowDefinition>
    </Grid.RowDefinitions>
    <TextBlock Foreground="White" x:Name="TaskName" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center">Product</TextBlock>
        <TextBlock Foreground="White" Grid.Row="1" x:Name="TaskInformation" FontSize="16" HorizontalAlignment="Center">Product</TextBlock>

    </Grid>
</Window>
